{
  "main": "index.js",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build --base=/app/",
    "serve": "vite preview",
    "dev:js": "rollup -c -w",
    "dev:css": "postcss app/css/index.css -o app/bundle.css -w",
    "build:pre:js": "rollup -c --environment NODE_ENV:production",
    "build:pre:css": "postcss app/css/index.css -o dist/bundle.css --env production",
    "build:post:copy": "cp index.html dist/index.html",
    "production": "npm run build && npm run prod:server"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "0.25.2",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "browser-sync": "2.26.7",
    "concurrently": "5.1.0",
    "cssnano": "^4.1.10",
    "import-http": "^0.3.1",
    "postcss": "7.0.36",
    "postcss-cli": "7.1.2",
    "postcss-easings": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "6.7.1",
    "rollup": "2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^3.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "vite": "2.x  || >2.0.0-alpha"
  }
}
